#include <stdio.h>
/**
 *main - print the result of the multiplication of two arguments
 *@argc: number of arguments
 *@argv: array contains the arguments
 *Return: Error
 */
int main(int argc, char **argv)
{
if (argc < 3)
{
printf("Error\n");
return (1);
}
int i;
int mul = 0;
for (i = 1 ; i < argc ; i++)
{
mul = atoi(argv[i]) * atoi(argv[i + 1]);
printf("%d\n", mul);
return (0);
}
}
